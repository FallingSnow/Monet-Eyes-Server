{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:l/recommended"],
    "plugins": ["react"],
    "parser": "babel-eslint",
    "rules": {
        "strict": 0
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "rules": {
        "no-console": "off",
        "no-duplicate-case": "off",
        "no-unused-vars": "warn"
    }
}
